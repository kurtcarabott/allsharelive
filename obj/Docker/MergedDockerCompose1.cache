services:
  allsharelive.api:
    build:
      context: /Users/kurtcarabott/DEV/AllShareLiveSystem/AllShareLive
      dockerfile: AllShareLive.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: AllShareLive.API
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44394'
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: allsharelive
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "AllShareLive.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 37043
      target: 80
    - published: 44394
      target: 443
    tty: true
    volumes:
    - /Users/kurtcarabott/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/kurtcarabott/.aspnet/https:/root/.aspnet/https:ro
    - /Users/kurtcarabott/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
version: '3.4'